# == Unity Generated Files and Folders ==

# The folder that stores the cached asset import metadata,
# local to the machine, and should not be tracked by Git.
/Library/

# The folder that stores project settings that are local to the machine,
# and should not be tracked by Git.
/Temp/
/Obj/
/Build/
/Builds/
/Logs/

# Autogenerated VS/MD/OSX project files
*.csproj
*.unityproj
*.sln
*.suo
*.sln.DotSettings
*.user
*.userprefs

# Unity-specific files
*.DS_Store
*.gz

# Asset Store meta data
.vs/
.idea/
.vscode/

# == Optional Exclusions ==

# Exclude generated executable files (optional, but recommended)
# You might want to include these if you are distributing builds via Git.
/Assets/AssetStoreTools/
/Assets/StreamingAssets/
